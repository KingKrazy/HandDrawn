function MenuGui::deleteTheora(%this)
{
MenuTheora.Delete();
%this.GenJoke();

error("Theora Deleted!");
}

function MenuGui::GenJoke(%this)
{
    //Load jokes
    %JokeOpt[1] = $Joke[getRandom(1,33)];
    %JokeOpt[2] = $Joke[getRandom(1,33)];

    %JokeOpt = %JokeOpt[getRandom(1,2)];
    %len = strlen(%JokeOpt);
    
    for (%i = 1; %i <= %len; %i++)
    {
        %tempStr = getSubStr(%JokeOpt, 0, %i);
        %time = %i * 10;
        %this.schedule(%time, "updateJokeText", %tempStr);
    }
}

