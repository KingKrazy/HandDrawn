function MissionTitleGui::onWake(%this)
{
DatabaseWindow.Extent.y = 0;
DataMake();
}

function DataMake()
{
if(DatabaseWindow.extent.y < 112)
{
DatabaseWindow.extent.y += 0.0001;
schedule(20, 0, "DataMake");
}
}
